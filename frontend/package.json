{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.2",
    "dotenv-webpack": "^8.1.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "web-vitals": "^2.1.4",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "scripts": {
    "cleanBuild": "rm -rf build/* && cp public/index.html build/index.html",
    "build:development": "npm run cleanBuild && NODE_ENV=development webpack --mode development",
    "build:preproduction": "npm run cleanBuild && NODE_ENV=preproduction webpack --mode preproduction",
    "build:production": "npm run cleanBuild && NODE_ENV=production webpack --mode production",
    "start:development": "NODE_ENV=development webpack serve --mode development --open",
    "start:preproduction": "NODE_ENV=preproduction webpack serve --mode preproduction",
    "start:production": "NODE_ENV=production webpack serve --mode production",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!axios)/\"",
    "test-pre-commit": "react-scripts test --watchAll=false --transformIgnorePatterns 'node_modules/(?!axios)/'",
    "eject": "react-scripts eject",
    "lint": "prettier --check .",
    "format": "prettier --write $(git diff --name-only --cached --diff-filter d | grep '.ts*' | sed 's/frontend\\(.*\\)/\\./' | xargs)"
  },
  "lint-staged": {
    "*": "prettier --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}
